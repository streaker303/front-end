.bx-wrapper {
    position: relative;
    margin: 0;
    padding: 0;
    *zoom: 1;
    -ms-touch-action: pan-y;
    touch-action: pan-y;
}
.bx-wrapper ul li {
    width:100%;
    display: block;
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
}
.bx-wrapper img {
    max-width: 100%;
    display: block;
}

