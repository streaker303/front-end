
body.white-theme,
.white-theme #app {
  height: 100%;
  background-color:rgba(0,0,0,0)!important;
  min-width: 1200px;
  background: none!important;
}
/* 弹层相关 */
.white-theme .confirm-box.el-message-box{
    border: 1px solid rgba(0,0,0,0);
    box-shadow: none;
    background: #fff;
}
.white-theme .confirm-box:before{
    content: '';
    position: absolute;
    left: -1px;
    top: -11px;
    width: 90px;
    height: 28px;
    background: none;
}
.white-theme .confirm-box.el-message-box .el-message-box__content{
    background: none;
    color: #333;
}
.white-theme .confirm-box.el-message-box .el-message-box__btns{
    padding: 0 20px 30px;
}
.confirm-box .el-message-box__header{
    height: 40px;
    padding: 11px 20px;
    box-sizing: border-box;
    font-size: 0;
    position: relative;
}
.white-theme .confirm-box .el-message-box__header .el-message-box__title{
    /*border-left: 4px solid #00E9FF;*/
    padding-left: 14px;
    color: #333;
    height: 18px;
    line-height: 18px;
    display: inline-block;
    position: relative;
}
.white-theme .confirm-box .el-message-box__header .el-message-box__title:before{
    width: 4px;
    height: 18px;
    content: ' ';
    position: absolute;
    left: 0px;
    top: 0;
    border-radius: 3px;
    background: #3f4f57;
}
.white-theme .confirm-box .el-message-box__headerbtn{
    top: 10px;
}
.white-theme .confirm-box .el-message-box__headerbtn .el-message-box__close{
    font-size: 18px;
    color: #333;
}
/*滚动条样式*/
/*修改滚动条样式*/
.white-theme div::-webkit-scrollbar,.white-theme li::-webkit-scrollbar,.white-theme ul::-webkit-scrollbar{
    width:4px;
    height:5px;
    /**/
}
.white-theme div::-webkit-scrollbar-track, .white-theme li::-webkit-scrollbar-track, .white-theme ul::-webkit-scrollbar-track{
    padding:0 3px;
    background: #fff;
    border-radius:8px;
}
.white-theme div::-webkit-scrollbar-thumb,.white-theme li::-webkit-scrollbar-thumb,.white-theme ul::-webkit-scrollbar-thumb{
    background: #496573;
    border-radius:14px;
}
.white-theme div::-webkit-scrollbar-thumb:hover,.white-theme li::-webkit-scrollbar-thumb:hover,.white-theme ul::-webkit-scrollbar-thumb:hover{
    background: #496573;
}
.white-theme div::-webkit-scrollbar-corner,.white-theme li::-webkit-scrollbar-corner,.white-theme ul::-webkit-scrollbar-corner{
    background: #179a16;
}
.white-theme textarea::-webkit-scrollbar{
    width:4px;
    height:5px;
}
.white-theme textarea::-webkit-scrollbar-track{
    padding:0 3px;
    background: #fff;
    border-radius:8px;
}
.white-theme textarea::-webkit-scrollbar-thumb{
    background: #496573;
    border-radius:14px;
}
.white-theme textarea::-webkit-scrollbar-thumb:hover{
    background: #496573;
}
.white-theme textarea::-webkit-scrollbar-corner{
    background: #179a16;
}

.white-theme .panel-wrapper::-webkit-scrollbar{
    width:3px;
    height:7px;
}
.white-theme .panel-wrapper::-webkit-scrollbar-thumb{
    background: #496573;
    border-radius:14px;
}

/* 项目页面整个背景 */
.white-theme #app{
    background: none !important;
    background-color:#e6e6e6 !important;
}
/* 顶部区域 */
.white-theme .top-bar {
    height: 60px !important;
    background: none !important;
    background-color:#37474f !important;
    box-shadow: 0px 6px 10px 0px  rgba(55, 71, 79, 0.25) !important;
}
.white-theme .second-view-wrapper{
    height:calc(100vh - 60px) !important;
}

.white-theme .second-box{
    padding-top:20px;
}
/* 搜索区域样式 */
.white-theme .event-content {
    padding: 20px 20px 20px 20px;
